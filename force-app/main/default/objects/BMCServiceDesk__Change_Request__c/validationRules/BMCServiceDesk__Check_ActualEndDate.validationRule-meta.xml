<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BMCServiceDesk__Check_ActualEndDate</fullName>
    <active>true</active>
    <description>This rule checks to make sure that Actual End Date comes always after Actual Start Date.</description>
    <errorConditionFormula>AND(NOT(OR(ISNULL(BMCServiceDesk__Actual_End_Date__c),ISBLANK(BMCServiceDesk__Actual_End_Date__c) ) ),OR( BMCServiceDesk__Actual_End_Date__c &lt; BMCServiceDesk__Actual_Start_Date__c,  BMCServiceDesk__Actual_End_Date__c = BMCServiceDesk__Actual_Start_Date__c) )</errorConditionFormula>
    <errorMessage>Actual End Date must be later than Actual Start Date</errorMessage>
</ValidationRule>
