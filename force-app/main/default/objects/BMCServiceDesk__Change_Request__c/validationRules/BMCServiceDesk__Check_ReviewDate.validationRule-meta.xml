<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BMCServiceDesk__Check_ReviewDate</fullName>
    <active>true</active>
    <description>This rule checks to make sure that Review Date comes always after Actual End Date.</description>
    <errorConditionFormula>AND(NOT(OR(ISNULL(BMCServiceDesk__Review_Date__c),ISBLANK(BMCServiceDesk__Review_Date__c) ) ),OR( BMCServiceDesk__Review_Date__c &lt; BMCServiceDesk__Actual_End_Date__c,  BMCServiceDesk__Actual_End_Date__c = BMCServiceDesk__Review_Date__c) )</errorConditionFormula>
    <errorMessage>The Review Date must be later than the Actual End Date</errorMessage>
</ValidationRule>
