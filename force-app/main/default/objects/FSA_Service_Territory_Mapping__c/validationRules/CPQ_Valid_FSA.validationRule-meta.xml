<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CPQ_Valid_FSA</fullName>
    <active>true</active>
    <errorConditionFormula>OR(NOT(REGEX(  FSA__c , &quot;[A-Z]\\d[A-Z]&quot;)) 
, LEN(FSA__c) !=3)</errorConditionFormula>
    <errorDisplayField>FSA__c</errorDisplayField>
    <errorMessage>Please enter a FSA with a A#A format.</errorMessage>
</ValidationRule>
