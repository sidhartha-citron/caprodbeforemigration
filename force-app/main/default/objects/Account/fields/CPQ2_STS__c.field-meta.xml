<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CPQ2_STS__c</fullName>
    <description>This is the Ship To Score for this Account</description>
    <externalId>false</externalId>
    <formula>if( CPQ2_HSO__c || CPQ2_GM__c == 0, null, ROUND((CPQ2_GMP__c + $Setup.CPQ_Settings__c.CPQ_Deal_Score_Buffer__c)*100,0))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Ship To Score</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
