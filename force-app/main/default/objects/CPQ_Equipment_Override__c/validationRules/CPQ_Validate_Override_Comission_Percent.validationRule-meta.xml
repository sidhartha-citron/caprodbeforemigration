<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CPQ_Validate_Override_Comission_Percent</fullName>
    <active>true</active>
    <errorConditionFormula>IF( CPQ_Override_Commission_Percent__c &lt; 0 || CPQ_Override_Commission_Percent__c &gt; 0.50, true, false)</errorConditionFormula>
    <errorDisplayField>CPQ_Override_Commission_Percent__c</errorDisplayField>
    <errorMessage>Override Commission Percent must be between 0% and 50%</errorMessage>
</ValidationRule>
