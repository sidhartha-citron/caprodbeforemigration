<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BMCServiceDesk__Compliant__c</fullName>
    <deprecated>false</deprecated>
    <description>This field stores the value if the problem is compliant or not</description>
    <externalId>false</externalId>
    <formula>IF(ISNULL(BMCServiceDesk__Due_Date_Time__c), &quot;YES&quot;, 
	( IF( ISNULL(BMCServiceDesk__Closed_Date_Time__c), ( IF(BMCServiceDesk__Due_Date_Time__c &gt;= NOW(), &quot;YES&quot;, &quot;NO&quot;)), 
		( IF(BMCServiceDesk__Due_Date_Time__c &gt;= BMCServiceDesk__Closed_Date_Time__c, &quot;YES&quot;, &quot;NO&quot;))
	))
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Compliant</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
