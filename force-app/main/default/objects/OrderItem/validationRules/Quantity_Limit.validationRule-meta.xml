<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quantity_Limit</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(ISBLANK(Quantity)) &amp;&amp; Quantity &lt;&gt; 1</errorConditionFormula>
    <errorDisplayField>Quantity</errorDisplayField>
    <errorMessage>Quantity can only be set to 1</errorMessage>
</ValidationRule>
