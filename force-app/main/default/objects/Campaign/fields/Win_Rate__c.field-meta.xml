<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Win_Rate__c</fullName>
    <description>Marketing Direction phase 1</description>
    <externalId>false</externalId>
    <formula>IF
(
OR(ISBLANK(NumberOfOpportunities),NumberOfOpportunities= 0), 0 ,(NumberOfWonOpportunities / NumberOfOpportunities)
)</formula>
    <label>Win Rate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Percent</type>
</CustomField>
