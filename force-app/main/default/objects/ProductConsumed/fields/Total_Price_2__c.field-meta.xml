<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Price_2__c</fullName>
    <description>there&apos;s a standard total price on product consumed but it cannot be added to report types. Thus creating a formula field to accomadate</description>
    <externalId>false</externalId>
    <formula>UnitPrice * QuantityConsumed</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total Price 2</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
