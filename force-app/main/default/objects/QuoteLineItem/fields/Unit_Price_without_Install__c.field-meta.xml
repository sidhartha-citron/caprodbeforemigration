<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Unit_Price_without_Install__c</fullName>
    <description>This field will be used in the One time table in the Conga template. We only want the install price to show up in the installation table,</description>
    <externalId>false</externalId>
    <formula>UnitPrice  -  Installation_Price__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Unit price - install price</inlineHelpText>
    <label>Unit Price without Install</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Currency</type>
</CustomField>
