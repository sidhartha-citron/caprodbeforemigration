<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CPQ_Validate_Commission_Multiplier</fullName>
    <active>true</active>
    <errorConditionFormula>IF( CPQ_Commission_Multiplier__c &lt; 0 ||  CPQ_Commission_Multiplier__c &gt; 10, true, false)</errorConditionFormula>
    <errorDisplayField>CPQ_Commission_Multiplier__c</errorDisplayField>
    <errorMessage>Commission Multiplier must be a value between 0 and 10</errorMessage>
</ValidationRule>
