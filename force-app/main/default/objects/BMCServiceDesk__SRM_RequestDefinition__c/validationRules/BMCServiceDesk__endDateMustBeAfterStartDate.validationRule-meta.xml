<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BMCServiceDesk__endDateMustBeAfterStartDate</fullName>
    <active>true</active>
    <description>End date must come after Start Date</description>
    <errorConditionFormula>AND(NOT(OR(ISNULL(BMCServiceDesk__EndDate__c),ISBLANK(BMCServiceDesk__EndDate__c) ) ), BMCServiceDesk__EndDate__c &lt; BMCServiceDesk__StartDate__c  )</errorConditionFormula>
    <errorMessage>The End Date must be later than the Start Date</errorMessage>
</ValidationRule>
