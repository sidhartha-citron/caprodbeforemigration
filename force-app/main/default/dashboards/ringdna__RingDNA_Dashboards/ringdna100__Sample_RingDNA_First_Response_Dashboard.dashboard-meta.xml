            <report>ringdna100__RingDNAReports/ringdna100__Time_to_Respond_to_Inbound_Leads</report>
            <showPercentage>true</showPercentage>
            <showValues>true</showValues>
            <sortBy>RowLabelAscending</sortBy>
            <useReportChart>false</useReportChart>
        </components>
    </middleSection>
    <rightSection>
        <columnSize>Medium</columnSize>
    </rightSection>
    <runningUser>sgurung@citronhygiene.com</runningUser>
    <textColor>#000000</textColor>
    <title>Sample RingDNA First Response Dashboard</title>
    <titleColor>#000000</titleColor>
    <titleSize>12</titleSize>
</Dashboard>
